<SwaggerApiProxySettings>
  <EndPoints>
    <EndPoint>
      <Id>Petstore</Id>
      <Url>https://raw.githubusercontent.com/swagger-api/swagger-spec/master/examples/v2.0/json/petstore-expanded.json</Url>
      <Namespace>Petstore</Namespace>
      <Suffix>WebProxy</Suffix>
      <BaseProxyClass>Swagger.WebApiProxy.Demo.Client.BaseProxy</BaseProxyClass>
      <ProxyConstructorSuffix>(Uri baseUrl) : base(baseUrl)</ProxyConstructorSuffix>

      <ParseOperationIdForProxyName>false</ParseOperationIdForProxyName>
    </EndPoint>
    <EndPoint>
      <Id>Demo</Id>
      <Url>http://localhost:54076/swagger/docs/1.0</Url>
      <Namespace>Demo</Namespace>
      <Suffix>WebProxy</Suffix>
      <BaseProxyClass>Swagger.WebApiProxy.Demo.Client.BaseProxy</BaseProxyClass>
      <ParseOperationIdForProxyName>true</ParseOperationIdForProxyName>
      <ProxyConstructorSuffix>(Uri baseUrl) : base(baseUrl)</ProxyConstructorSuffix>

    </EndPoint>
  </EndPoints>
</SwaggerApiProxySettings>